 {
        "family": "edudate-task-def",
        "networkMode": "awsvpc",
        "taskRoleArn": "arn:aws:iam::479788010133:role/execCommandRole",
        "executionRoleArn": "arn:aws:iam::479788010133:role/execCommandRole",
        "containerDefinitions": [
            {
                "name": "edudate-web",
                "image": "479788010133.dkr.ecr.us-west-2.amazonaws.com/edudate:latest",
                "portMappings": [
                    {
                        "containerPort": 80,
                        "hostPort": 80,
                        "protocol": "tcp"
                    },
                    {
                        "containerPort": 8101,
                        "hostPort": 8101,
                        "protocol": "tcp"
                    }, 
                    {
                        "containerPort": 8201,
                        "hostPort": 8201,
                        "protocol": "tcp"
                    },
                    {
                        "containerPort": 3306,
                        "hostPort": 3306,
                        "protocol": "tcp"
                    },
                    {
                        "containerPort": 22,
                        "hostPort": 22,
                        "protocol": "tcp"
                    }
                ],
                "essential": true,

                "command":["/bin/sh -c \"nginx -g 'daemon off;'\""]       
            }
        ],
        "requiresCompatibilities": [
            "FARGATE"
        ],
        "cpu": "256",
        "memory": "512"
}