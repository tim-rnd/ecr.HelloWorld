 {
        "family": "hahlabs-mysql-task-def",
        "networkMode": "awsvpc",
        "taskRoleArn": "arn:aws:iam::479788010133:role/execCommandRole",
        "executionRoleArn": "arn:aws:iam::479788010133:role/execCommandRole",
        "containerDefinitions": [
            {
                "name": "hahlabs-mysql",
                "image": "479788010133.dkr.ecr.us-west-2.amazonaws.com/hahlabs/mysql:dev",
                "portMappings": [
                    {
                        "containerPort": 3306,
                        "hostPort": 3306,
                        "protocol": "tcp"
                    },
                    {
                        "containerPort": 22,
                        "hostPort": 22,
                        "protocol": "tcp"
                    }
                ],
                "essential": true,
                "command": [
                    "/usr/bin/sudo service ssh start"
                ],
                "entryPoint": [
                    "/bin/sh", "-c" ," /usr/bin/sudo service ssh start && tail -f /dev/null"
                ],
                "environment": [],
                "mountPoints": [],
                "volumesFrom": []
            }
        ],
        "requiresCompatibilities": [
            "FARGATE"
        ],
        "cpu": "1024",
        "memory": "4096"
}